page{
  background: #f5f5f5;
}

.loginBox{
  height: 800rpx;
  padding-top: 50rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.logo image{
  width: 204rpx;
  height: 280rpx;
}
.registerBtn {
  width: 130rpx;
  height: 50rpx;
  font-size: 22rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #FF0000;
  border: 1rpx solid #FF0000;
  border-radius: 22rpx;
}
.loginInput {
  width: 428rpx;
  height: 50rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0,0.1);
  margin-top: 10rpx;
  position: relative;
}
.loginInput input{
  width: 380rpx;
  font-size: 20rpx;
  position: absolute;
  top: 0;
  right: 0;
}
.loginInput image {
  width: 27rpx;
  height: 33rpx;
}
.forget {
  font-size: 18rpx;
  color: rgba(17, 131, 227,0.4);
  text-decoration: underline;
  display: flex;
  justify-content: flex-end;
}
.loginBtn {
  width: 64rpx;
  height: 65rpx;
}
.footImg {
  width: 750rpx;
  height: 192rpx;
  position: absolute;
  bottom: 0;
} 